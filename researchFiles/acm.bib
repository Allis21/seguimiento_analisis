author = {Maram, Sai Siddartha and Kleinman, Erica and Villareale, Jennifer and Zhu, Jichen and Seif El-Nasr, Magy},
title = {"Ah! I see" - Facilitating Process Reflection in Gameplay through a Novel Spatio-Temporal Visualization System},
