url = {https://doi.org/10.1145/3696008},
doi = {10.1145/3696008},
abstract = {According to the United Nations (UN), a “continuous, participatory, and inclusive urban planning process should be the starting point and framework for improving population living conditions,” and this research emerges from examining trends in smart cities. It explores the impact of collective intelligence, geographical information systems, and cognitive systems as a way of supporting the decision-making of local governments (municipalities) to face the challenges announced by the UN in the Urban Agenda 2017. This methodology employs an exploratory approach using the participatory action research method. The model was tested in three municipalities in Ecuador using a technological and collaborative platform called Cognitive Urban Planning (PUC). The results highlighted improvements in the decision-making process in urban planning, since the interactive participation processes through the PUC allowed for the suggestion of a group of strategies for an emerging neighborhood plan developed with the citizens working together in the municipality in real-time. In conclusion, the effects of the decision-making plan could be applied to improve and enhance community participation through city co-creation, supporting the decision-making process of municipalities in a way that promotes inclusive urban planning and stakeholders' awareness, as the recommended plans are supported by the desires and needs raised by citizens.},
journal = {Digit. Gov.: Res. Pract.},
month = oct,
articleno = {35},
numpages = {30},
keywords = {Smart cities, artificial intelligence, citizen engagement, collective intelligence, collaborative urban planning model}
}

@inproceedings{10.1145/3689535.3689543,
author = {Addo, Salomey Afua and Sentance, Sue},
title = {Exploring Computing Teachers' Readiness to Teach AI in Secondary Schools},
year = {2024},
isbn = {9798400711770},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3689535.3689543},
doi = {10.1145/3689535.3689543},
abstract = {Artificial intelligence (AI) is significantly impacting how we live, and the increased capabilities of generative AI applications have positioned AI firmly in the public domain. There is a growing interest in what AI might look like as a subject within the K-12 curriculum, whilst research on teachers’ readiness for teaching AI is as yet limited. This paper describes a qualitative study investigating teachers’ readiness to teach AI in secondary education. The interview study involved eight computing teachers with varying teaching experiences. We used reflexive thematic analysis for themes development. Findings suggest several indicators of teachers’ readiness, including attitudes, prior AI experience, professional development, and access to quality resources. This paper contributes to ongoing debates about how to best support teachers to be ready to teach AI effectively at the school level.},
booktitle = {Proceedings of the 2024 Conference on United Kingdom &amp; Ireland Computing Education Research},
articleno = {17},
numpages = {1},
keywords = {K-12 education, artificial intelligence, computing education, teacher readiness},
location = {Manchester, United Kingdom},
series = {UKICER '24}
}

@inproceedings{10.1145/3670013.3670035,
author = {Tiongco, Ryan G. and Casaje, June Grace S and Dela Cruz, Edellita A. and Ruiz, Mary Jane G. and Fabores, Sheila May A. and Prudente, Maricar S},
title = {Scientific Literacy Skills and Perceptions about STEM Curriculum Using Web-based Applications},
year = {2024},
isbn = {9798400717062},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3670013.3670035},
doi = {10.1145/3670013.3670035},
abstract = {The Philippines' K-12 education began in 2013 under President Benigno Aquino III to meet global standards and offer a more comprehensive education. The STEM track in Senior High School (SHS) emphasizes science, technology, engineering, and mathematics but faces challenges in effective implementation within the K-12 curriculum in the Philippines. School administrators should regularly evaluate the K12 STEM curriculum to ensure alignment with international standards and students' scientific literacy (Rogayan, 2021).This study aimed to (1) assess the Scientific Literacy of K-12 STEM curriculum graduates through the Test of Scientific Literacy Skills and Scientific Reasoning (TOSLS:SR) (2) describe the implementation of the STEM curriculum through the students’ perceptions of their teachers, school administrators and school resources and facilities and, (3) elucidate the affordances and challenges in learning as experienced by the Grade 12 STEM student graduates using the Curriculum Implementation Survey (CIS). This study utilized the descriptive quantitative research design with the use of online survey questionnaires through Google Forms to assess the scientific literacy of K-12 STEM curriculum graduates through the Test of Scientific Literacy Skills and Scientific Reasoning. Graduates of K-12 STEM from 5 different institutions were targeted as respondents. One hundred sixty-five (165) graduates answered the Test of Scientific Literacy Skills and Scientific Reasoning while one hundred eighty-one (181) graduates answered the curriculum implementation survey. Quantitative data were computed using Jamovi Cloud. Results showed that mastered 7 out of 9 scientific literacy skills in the TOSLS. In the CIS, the mean of the overall responses is approximately 3.485, indicating a generally positive perception of the teachers' capabilities and attitudes towards science, technology, engineering, and mathematics (STEM) subjects. Moreover, the weighted mean of 3.44 indicates a generally positive perception of the school facilities for STEM education. In conclusion, among 5 different institutions included in this study, the implementation of STEM curriculum elicited a positive response from the STEM Curriculum graduates.CCS CONCEPTS • Social and professional topics • Social and professional topics∼K-12 education},
booktitle = {Proceedings of the 2024 15th International Conference on E-Education, E-Business, E-Management and E-Learning},
pages = {170–175},
numpages = {6},
keywords = {Google Forms, Jamovi, STEM Curriculum, Scientific Literacy, Web-based Applications},
location = {Fukuoka-shi, Japan},
series = {IC4E '24}
}

@inproceedings{10.1145/3612783.3612802,
author = {Higuera, Marta and Mac\'{\i}as, Jos\'{e} Antonio},
title = {Automatic Generation of Empathy Maps},
year = {2024},
isbn = {9798400707902},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3612783.3612802},
doi = {10.1145/3612783.3612802},
abstract = {Empathy Maps (EMs) are broadly used in UX and Design Thinking. They can be seen as a useful tool to help developers get a better understanding of the user's requirements. During the interaction design, it is important to have a thoroughgoing knowledge of what the user wants, which might be difficult to gather or portrait to others somehow. In this sense, EMs provide a visual representation of the user's information, attitude, and behavior. Nevertheless, creating such EMs can become tedious, as this usually requires the involvement of a group of people, with expert knowledge, in-deep analyzing the information elicited from the user. In this paper, we present a tool for the automatic generation of EMs. This comprises a web tool aimed to automatically generate an Empathy Map from an audio interview with the user by using machine learning technology. More specifically, this paper reports on the architecture, a use case, and a validation with numerical evidence to corroborate the feasibility of the proposal presented.},
