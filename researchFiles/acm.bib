@inproceedings{10.1145/3649217.3653564,
author = {Fine, Benjamin T.},
title = {Competency and Equity Driven Grading System for Computer Science Curriculum},
year = {2024},
isbn = {9798400706004},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3649217.3653564},
doi = {10.1145/3649217.3653564},
abstract = {Traditional grading practices have been criticized for their lack of accuracy and fairness in assessing student learning by using a one-size-fits-all approach that does not account for differences in students' backgrounds, experiences, or learning needs. This study examines the use of a hybrid of competency- and equity-based grading style across the computer science curriculum. Specifically, this study explores the experiences of students and educators in implementing this grading system and assesses its initial impact on student success and attitudes toward learning through observational data. Data was collected over fall, spring, and summer terms that included in-person and remote instruction. The courses studied range from an introductory course to upper-level courses (both required and elective). We discuss the commonly brought-up issue of feasibility and its impact on faculty and students that are already tasked with many responsibilities by reviewing the number of student interactions that took place outside of scheduled class time. We see that the amount of time required to assess the students does increase slightly compared to traditional assessment but the number of student interactions increases, thus leading to more engagement. By examining and refining this approach to assessment and grading, we contribute to a deeper understanding of how competency- and equity-based grading can support more equitable and inclusive educational practices that help students achieve a high level of content understanding.},
booktitle = {Proceedings of the 2024 on Innovation and Technology in Computer Science Education V. 1},
pages = {311–317},
numpages = {7},
keywords = {competency-based grading, equity-based grading, inclusive educational practices, universal design instruction},
location = {Milan, Italy},
series = {ITiCSE 2024}
}

@article{10.1145/3649821,
author = {Zilberstein, Noam and Saliling, Angelina and Silva, Alexandra},
title = {Outcome Separation Logic: Local Reasoning for Correctness and Incorrectness with Computational Effects},
year = {2024},
issue_date = {April 2024},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {8},
number = {OOPSLA1},
url = {https://doi.org/10.1145/3649821},
doi = {10.1145/3649821},
abstract = {Separation logic’s compositionality and local reasoning properties have led to significant advances in scalable static analysis. But program analysis has new challenges—many programs display computational effects and, orthogonally, static analyzers must handle incorrectness too. We present Outcome Separation Logic (OSL), a program logic that is sound for both correctness and incorrectness reasoning in programs with varying effects. OSL has a frame rule—just like separation logic—but uses different underlying assumptions that open up local reasoning to a larger class of properties than can be handled by any single existing logic. Building on this foundational theory, we also define symbolic execution algorithms that use bi-abduction to derive specifications for programs with effects. This involves a new tri-abduction procedure to analyze programs whose execution branches due to effects such as nondeterministic or probabilistic choice. This work furthers the compositionality promised by separation logic by opening up the possibility for greater reuse of analysis tools across two dimensions: bug-finding vs verification in programs with varying effects.},
journal = {Proc. ACM Program. Lang.},
month = apr,
articleno = {104},
numpages = {29},
keywords = {Incorrectness, Outcome Logic, Separation Logic}
}

