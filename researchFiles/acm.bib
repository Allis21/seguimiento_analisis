articleno = {18},
numpages = {8},
keywords = {Design Thinking, Empathy Map, Intelligent User Interfaces, Usability, User Experience},
location = {Lleida, Spain},
series = {Interacci\'{o}n '23}
}

@inproceedings{10.1145/3691573.3691587,
