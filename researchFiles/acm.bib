url = {https://doi.org/10.1145/3661904.3661911},
doi = {10.1145/3661904.3661911},
abstract = {This study highlighted the assessed influence of self-efficacy in computer programming and grades in programming courses in C language as predictors for the object-oriented programming performance in Java of the (306) Bachelor of Science in Information Technology (BSIT) and Bachelor of Science in Computer Science (BSCS) students. The Computer Programming Self-Efficacy Scale (CPSES) was utilized for the online survey. The students were selected through purposive sampling. The grades of the students in fundamentals and intermediate programming in C language were also collected. The data was preprocessed to ensure quality. The data was analyzed using Pearson correlation and multiple linear regression analysis using RStudio and Google Colaboratory. The result highlights that intermediate programming grade in C has no significant influence to predict OOP performance in Java, but the grade in fundamentals of programming in C language and self-efficacy using CPSES are good contributing factors to the prediction model. The result of this study offers educators a powerful tool for curriculum development and pedagogical strategies to better help students and future research studies in this domain.},
booktitle = {Proceedings of the 2024 10th International Conference on Education and Training Technologies},
pages = {101–107},
numpages = {7},
keywords = {C Programming, Java, Object Oriented Programming, Predictors, Self-efficacy},
location = {Macau, China},
series = {ICETT '24}
}

@inproceedings{10.1145/3613905.3637130,
author = {Santana, Vagner Figueredo de and Quigley, Lauren Thomas and Coates, Jessica and Graves, Maxine},
title = {Computational Notebook as a Tool for Supporting Black Engineers Non-Profit Organizations in the Context of Workforce Participation},
year = {2024},
isbn = {9798400703317},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3613905.3637130},
doi = {10.1145/3613905.3637130},
abstract = {Data is frequently referred to as the ‘new oil’ due to its high strategic value and the possibilities it brings. Additionally, its use by powerful organizations has the potential to help, but also to cause harm and function as an additional oppressive mechanism towards vulnerable communities. Hence, there is a need for providing easier methods for people and organizations to access, analyze, and interpret reliable, publicly available data (e.g., U.S. Census), and create data narratives that reflect their social position in the society and inform how to improve that position. In this case study, we document our journey on using computational notebooks to increase autonomy for organizations tackling racial and social justice issues by making meaningful use of publicly available data. The project considered a participatory action research approach run in partnership with a non-profit organization supporting Black engineers. The reported case study involved participants representing staff and volunteer leadership of the organization. We expect that the presented reflections help move towards a more equitable way of performing data analysis while using popular and freely available technologies.},
booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
articleno = {512},
numpages = {9},
keywords = {Computational Notebooks, Data Action, Jupyter Notebook, Participatory Action Research, Racial Justice., Social Justice},
location = {Honolulu, HI, USA},
series = {CHI EA '24}
}

@inproceedings{10.1145/3629296.3629356,
author = {Huang, Shian and Xu, Jing and Sang, Qi and Shao, Zeguo},
title = {Research on the digital literacy model construction and practice of higher vocational students in the digital era},
year = {2024},
isbn = {9798400709111},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3629296.3629356},
doi = {10.1145/3629296.3629356},
abstract = {Entering the digital age, digital literacy has become one of the essential knowledge and key skills for talents. First of all, this paper sorts out the research status and the existing framework of digital literacy at home and abroad, conducts comparative analysis and research, puts forward the index reference frame, and preliminarily constructs the evaluation index of digital literacy of higher vocational students. Then, the evaluation index of digital literacy is verified by factor analysis, and the data results prove that the evaluation index constructed in this paper has certain rationality. On this basis, the weight of the evaluation index is calculated, and thus the formal digital literacy evaluation index of higher vocational students is obtained. Finally, according to the digital literacy evaluation index of higher vocational students' digital literacy level questionnaire, the Su Xichang area vocational students digital literacy level is analyzed, further verify the higher vocational students' digital literacy components and the rationality and scientific evaluation model, to enhance the level of digital education in higher vocational colleges to provide certain reference.},
