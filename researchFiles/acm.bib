abstract = {Including young people with Attention and Hyperactive Disorders (ADHD) in technology co-design with physical computing can empower them differently. This paper focuses on how to give ADHD learners new means of expression and promoting self-agency. It starts presenting recent research on tools for collaboratively engaging young learners in co-design with physical computing. Then, it overviews the ADHD learners’ requirements to enable them to participate fully, presenting the tool prototype designed based on a strength-based perspective. It concludes with the future steps to improve the tool as an empowerment opportunity for ADHD learners.},
booktitle = {Proceedings of the 2024 International Conference on Advanced Visual Interfaces},
articleno = {113},
