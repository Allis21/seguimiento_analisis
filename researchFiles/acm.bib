@inproceedings{10.1145/3637543.3652980,
author = {Buckley, James H. and Buhler, Jeremy and Chamberlain, Roger D.},
title = {The Advanced Particle-astrophysics Telescope (APT): Computation in Space},
year = {2024},
isbn = {9798400704925},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
