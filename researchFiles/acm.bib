series = {SIGCSETS 2025}
}

@inproceedings{10.1145/3649405.3659494,
author = {Pereira Nunes, Bernardo and Kaur, Guddu and Chan, April and Sharpe, Sue and Soto Ruidias, Rosa R.},
title = {Exploring Educational Escape Room as an Assessment Tool for Computer Science Courses},
