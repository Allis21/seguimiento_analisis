@inproceedings{10.1145/3698322.3698323,
author = {Bertels, Anja and Deimel, Dominik},
title = {Improving Coding Workshops: A Pattern Collection},
year = {2024},
isbn = {9798400716836},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3698322.3698323},
