@inproceedings{10.1145/3644523.3644539,
author = {Su, Yinsheng and Zeng, Xiangyu and Zhao, Ligang and Gao, Qin},
title = {Modeling and Topology Analysis of Computational Task Networks for Power System Simulation},
year = {2024},
isbn = {9798400709517},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3644523.3644539},
doi = {10.1145/3644523.3644539},
abstract = {With the development of software and hardware technology, the scale and performance of power system simulation is improved significantly. Current development of power system simulation programs is mainly an engineering problem. To play a more important role in the new generation of power system with deeper coupling between information and physics, basic theoretical research is urgently needed. In this paper, existing power flow and electromechanical transient simulation tasks are modelled as nodes for the basic calculation actions and connecting node to construct a computational task network for power system simulation. Several topological characteristics of the computational task network are analyzed, and the relationship between them and the typical numerical computing performance is discussed. This paper attempts to behave as the starting point of analyzing the power system simulation performance and determining the improvement path based on the complex network theory.},
