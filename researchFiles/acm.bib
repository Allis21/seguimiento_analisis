articleno = {24},
numpages = {2},
location = {Manchester, United Kingdom},
series = {UKICER '24}
}

@inproceedings{10.1145/3641554.3701967,
author = {Kneisel, Tristan and Radtke, Elias and Schmellenkamp, Marko and Vehlken, Fabian and Zeume, Thomas},
